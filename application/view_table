select `babes_vaibility_calender`.`staff_id` AS `staff_id`,`babes_vaibility_calender`.`day` AS `day`,
if((`babes_vaibility_calender`.`start_time_zone` = 'PM') and (`babes_vaibility_calender`.`start_time`<'12:00:00'),addtime(`babes_vaibility_calender`.`start_time`,'12:00:00') , if ((`babes_vaibility_calender`.`start_time_zone` = 'AM') and (`babes_vaibility_calender`.`start_time`>='12:00:00'),addtime(`babes_vaibility_calender`.`start_time`,'-12:00:00'),`babes_vaibility_calender`.`start_time`)) AS `st_time`,
if((`babes_vaibility_calender`.`end_time_zone` = 'PM') and (`babes_vaibility_calender`.`end_time`<'12:00:00'),addtime(`babes_vaibility_calender`.`end_time`,'12:00:00') , if ((`babes_vaibility_calender`.`end_time_zone` = 'AM') and (`babes_vaibility_calender`.`end_time`>='12:00:00'),addtime(`babes_vaibility_calender`.`end_time`,'-12:00:00'),`babes_vaibility_calender`.`end_time`)) AS `ed_time`
,`babes_vaibility_calender`.`start_time` AS `start_time`,`babes_vaibility_calender`.`start_time_zone` AS `start_time_zone`,`babes_vaibility_calender`.`end_time` AS `end_time`,`babes_vaibility_calender`.`end_time_zone` AS `end_time_zone`,`babes_vaibility_calender`.`remarks` AS `remarks` from `babes_vaibility_calender`

select `babes_calender_events`.`staff_id` AS `staff_id`,`babes_calender_events`.`date` AS `date`,if(((`babes_calender_events`.`start_time_zone` = 'PM') and (`babes_calender_events`.`start_time` < '12:00:00')),addtime(`babes_calender_events`.`start_time`,'12:00:00'),if(((`babes_calender_events`.`start_time_zone` = 'AM') and (`babes_calender_events`.`start_time` >= '12:00:00')),addtime(`babes_calender_events`.`start_time`,'-12:00:00'),`babes_calender_events`.`start_time`)) AS `st_time`,if(((`babes_calender_events`.`end_time_zone` = 'PM') and (`babes_calender_events`.`end_time` < '12:00:00')),addtime(`babes_calender_events`.`end_time`,'12:00:00'),if(((`babes_calender_events`.`end_time_zone` = 'AM') and (`babes_calender_events`.`end_time` >= '12:00:00')),addtime(`babes_calender_events`.`end_time`,'-12:00:00'),`babes_calender_events`.`end_time`)) AS `ed_time`,`babes_calender_events`.`start_time` AS `start_time`,`babes_calender_events`.`start_time_zone` AS `start_time_zone`,`babes_calender_events`.`end_time` AS `end_time`,`babes_calender_events`.`end_time_zone` AS `end_time_zone` from `babes_calender_events`
